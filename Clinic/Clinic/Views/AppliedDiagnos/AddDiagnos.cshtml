@model Clinic.Models.ClinicModels.AppliedDiagnos

@{
    ViewData["Title"] = "AddDiagnos";
}

<h2>AddDiagnos</h2>

<h4>AppliedDiagnos</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AddDiagnos" asp-controller="AppliedDiagnos">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
          
            <div class="form-group">
                <label  asp-for="AppointmentId" class="control-label"></label>
                <input   value="@ViewBag.AppointmentId" asp-for="AppointmentId" />

            </div>
            <div class="form-group">
                <label asp-for="DiagnosId" class="control-label"></label>
                <select asp-for="DiagnosId" class="form-control" asp-items="ViewBag.DiagnosId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

